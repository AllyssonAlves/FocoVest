# Environment Variables para Desenvolvimento Local
# Valores seguros para desenvolvimento

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ENVIRONMENT=development

# Security Configuration (desenvolvimento apenas)
REACT_APP_ENCRYPTION_KEY=dev-key-not-for-production
REACT_APP_CSP_NONCE=dev-nonce

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ENABLE_DEBUG=true

# Rate Limiting (mais permissivo para dev)
REACT_APP_MAX_LOGIN_ATTEMPTS=10
REACT_APP_LOGIN_COOLDOWN_MINUTES=5

# File Upload
REACT_APP_MAX_FILE_SIZE_MB=10
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf