# Environment Variables para Produção (GitHub Pages)
# IMPORTANTE: Não commitar valores reais no repositório

# API Configuration
REACT_APP_API_URL=https://api.focovest.com.br
REACT_APP_ENVIRONMENT=production

# Security Configuration
REACT_APP_ENCRYPTION_KEY=your-secure-encryption-key-here
REACT_APP_CSP_NONCE=your-csp-nonce-here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_DEBUG=false

# External Services
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Rate Limiting
REACT_APP_MAX_LOGIN_ATTEMPTS=5
REACT_APP_LOGIN_COOLDOWN_MINUTES=15

# File Upload
REACT_APP_MAX_FILE_SIZE_MB=5
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf