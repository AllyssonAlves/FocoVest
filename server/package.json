{
  "name": "focovest-server",
  "version": "1.0.0",
  "description": "Backend API para a plataforma FocoVest",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:simple": "nodemon --exec ts-node src/simpleServer.ts",
    "dev:fast": "nodemon --exec ts-node src/fastServer.ts",
    "secure": "nodemon --exec ts-node src/secureServer.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "seed:force": "ts-node src/scripts/seedDatabase.ts --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "quality": "ts-node src/scripts/codeQuality.ts",
    "optimize": "npm run lint:fix && npm run quality && npm run build"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/ioredis": "^4.28.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^2.0.3",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "request-ip": "^3.3.0",
    "ua-parser-js": "^1.0.38",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.6.2",
    "@types/request-ip": "^0.0.41",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
